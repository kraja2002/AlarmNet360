pipeline {
agent any
environment {
 dotnet ='C:\\Program Files (x86)\\dotnet\\'
}
    
stages {
stage ('Checkout') {
            steps {
                 git credentialsId: 'userId', url: 'https://github.com/kraja2002/SampleCliApp',branch: 'master'
            }
}
stage('Restore packages'){
   steps{
     {
     bat "C"\\Program Files\\Nuget.exe" restore SampleCliApp.sin"
     }
   }
  
stage('Clean') {
      steps {
            bat 'dotnet clean'
       }
    }
stage('Build') {
     steps {
            bat 'dotnet build --configuration Release'
      }
   }
stage('Pack') {
     steps {
           bat 'dotnet pack --no-build --output nupkgs'
      }
   }
stage('Publish') {
      steps {
           bat "dotnet nuget push **\\nupkgs\\*.nupkg -k yourApiKey -s http://myserver/artifactory/api/nuget/nuget-internal-stable/com/sample"
       }
   }
 }
}
